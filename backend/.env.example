# 通义千问 API Key
# 从阿里云百炼平台获取: https://bailian.console.aliyun.com/
DASHSCOPE_API_KEY=your_api_key_here

# 服务配置
HOST=0.0.0.0
PORT=8000

# 模型配置 (可选: qwen-max, qwen-plus, qwen-turbo)
QWEN_MODEL=qwen-plus

# 微信小程序配置
WECHAT_APPID=your_wechat_appid
WECHAT_SECRET=your_wechat_secret

# MySQL 配置
DB_HOST=your_mysql_host
DB_PORT=3306
DB_USER=your_mysql_user
DB_PASSWORD=your_mysql_password
DB_NAME=ai_advisor

# Token 过期时间（天）
ACCESS_TOKEN_EXPIRES_DAYS=7
REFRESH_TOKEN_EXPIRES_DAYS=30

# Redis 配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_change_me

# PostgreSQL 配置
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=ai_shopping
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password_change_me
