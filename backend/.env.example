# 通义千问 API Key
# 从阿里云百炼平台获取: https://bailian.console.aliyun.com/
DASHSCOPE_API_KEY=your_api_key_here

# 服务配置
HOST=0.0.0.0
PORT=8000

# 模型配置 (可选: qwen-max, qwen-plus, qwen-turbo)
QWEN_MODEL=qwen-plus

# Redis 配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_change_me

# PostgreSQL 配置
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=ai_shopping
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password_change_me
